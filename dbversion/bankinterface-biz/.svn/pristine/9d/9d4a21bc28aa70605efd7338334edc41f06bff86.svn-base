package com.yihuisoft.bankinterfacebiz.entity;

import java.io.Serializable;
import java.util.Date;

/**
 * 与数据库数据对应
 *
 * @author isst
 * @version v1.0.0
 */
public class BankMapping implements Serializable {
    /**
     * id
     */
    private Long id;
    /**
     * 交易码
     */
    private String interfaceNo;
    /**
     * 备注
     */
    private String remarks;
    /**
     * 创建人
     */
    private String writer;
    /**
     * 创建时间
     */
    private Date createDate;
    /**
     * 修改时时间
     */
    private Date modifyTime;
    /**
     * 发送数据（发送报文）
     */
    private String send;
    /**
     * 接收数据（接收报文）
     */
    private String receive;

    public BankMapping() {
    }

    public BankMapping(Long id, String interfaceNo, String remarks, String writer, Date createDate, Date modifyTime, String send, String receive) {
        this.id = id;
        this.interfaceNo = interfaceNo;
        this.remarks = remarks;
        this.writer = writer;
        this.createDate = createDate;
        this.modifyTime = modifyTime;
        this.send = send;
        this.receive = receive;
    }

    public BankMapping(String interfaceNo, String remarks, String writer, Date createDate, Date modifyTime, String send, String receive) {
        this.interfaceNo = interfaceNo;
        this.remarks = remarks;
        this.writer = writer;
        this.createDate = createDate;
        this.modifyTime = modifyTime;
        this.send = send;
        this.receive = receive;
    }

    public BankMapping(String interfaceNo, String remarks, String writer, String send, String receive) {
        this.interfaceNo = interfaceNo;
        this.remarks = remarks;
        this.writer = writer;
        this.send = send;
        this.receive = receive;
    }

    public BankMapping(Long id, String interfaceNo, String remarks, String writer, String send, String receive) {
        this.id = id;
        this.interfaceNo = interfaceNo;
        this.remarks = remarks;
        this.writer = writer;
        this.send = send;
        this.receive = receive;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getInterfaceNo() {
        return interfaceNo;
    }

    public void setInterfaceNo(String interfaceNo) {
        this.interfaceNo = interfaceNo;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }

    public String getWriter() {
        return writer;
    }

    public void setWriter(String writer) {
        this.writer = writer;
    }

    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }

    public Date getModifyTime() {
        return modifyTime;
    }

    public void setModifyTime(Date modifyTime) {
        this.modifyTime = modifyTime;
    }

    public String getSend() {
        return send;
    }

    public void setSend(String send) {
        this.send = send;
    }

    public String getReceive() {
        return receive;
    }

    public void setReceive(String receive) {
        this.receive = receive;
    }
}